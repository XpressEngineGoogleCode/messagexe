<query id="insertAuthentication" action="insert">
	<tables>
		<table name="authentication" />
	</tables>
	<columns>
		<column name="authentication_srl" var="authentication_srl" notnull="notnull" />
		<column name="clue" var="clue" notnull="notnull" />
		<column name="authcode" var="authcode" notnull="notnull" />
		<column name="country" var="country" />
		<column name="authcode_pass" var="authcode_pass" default="N" />
		<column name="authcode_ban" var="authcode_ban" default="N" />
		<column name="send_time" var="send_times" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
